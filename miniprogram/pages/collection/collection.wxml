<view class='collection-list'>
  <block wx:for="{{cuisineList}}" wx:for-item="cuisine" wx:key="index">
    <cuisine_item index="{{index}}" cuisine="{{cuisine.cuisine}}" data-cuisine_id='{{cuisine.cuisine._id}}' bindtap='_toCuisineDetail'></cuisine_item>
  </block>
</view>
<view wx:if='{{cuisineList == null}}' class='collection-none'>
  <image class='none-img' src='cloud://recipes.7265-recipes/image/index/zanwushuju.png'></image>
  <text class='none-title'>暂无数据</text>
</view>
<van-toast id="van-toast" />