<!--miniprogram/pages/searchList/searchList.wxml-->
<view class='recipes-search'>
  <input class="search-input" type="text" placeholder='搜索菜谱'></input>
  <icon class="search-icon" type="search" size="16"></icon>
</view>
<view class='cuisine-list'>
  <block wx:for="{{cuisineList}}" wx:for-item="cuisine" wx:key="index">
    <cuisine_item index="{{index}}" cuisine="{{cuisine}}" data-cuisine_id='{{cuisine._id}}' bindtap='_toCuisineDetail'></cuisine_item>
  </block>
</view>
<van-toast id="van-toast" />